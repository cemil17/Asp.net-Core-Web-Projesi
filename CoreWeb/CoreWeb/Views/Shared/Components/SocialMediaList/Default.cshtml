@using EntityLayer.Concrete
@model List<SocialMedia>


<div class="social-nav">
	<nav role="navigation">
		<ul class="nav justify-content-center">
			@foreach (var item in Model)
			{
				<li class="nav-item"><a class="nav-link" href="@item.URL"><i class="@item.Icon"></i><span class="menu-title sr-only">@item.Media</span></a></li>
			}
		</ul>
	</nav>
</div>



